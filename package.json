{
	"name": "vite-plugin-html-inject",
	"version": "1.1.3",
	"description": "Split your index.html into multiple files and inject them where ever you want!",
	"files": [
		"dist"
	],
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "npx unbuild",
		"prepack": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/nonebiotope/vite-plugin-html-inject.git"
	},
	"keywords": [
		"vite",
		"vite-plugin",
		"load html",
		"split html",
		"inject html",
		"html",
		"html-loader",
		"loader"
	],
	"author": "Nikita 'donnikitos' Nitichevski <me@donnikitos.com> (https://donnikitos.com/), NoneBiotope",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/nonebiotope/vite-plugin-html-inject/issues"
	},
	"homepage": "https://github.com/nonebiotope/vite-plugin-html-inject#readme",
	"devDependencies": {
		"@types/node": "^25.2",
		"typescript": "^5.9",
		"unbuild": "^3.6",
		"vite": "^7.3"
	},
	"packageManager": "yarn@4.0.2"
}
